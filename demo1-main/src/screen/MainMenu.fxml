<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<AnchorPane prefHeight="190.0" prefWidth="346.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="screen.MainMenuController">
    <ImageView fitHeight="800.0" fitWidth="1450.0">
        <image>
            <Image url="view/nen.gif" />
        </image>
    </ImageView>
    <children>
        <VBox alignment="CENTER" layoutX="5.0" layoutY="6.0" prefHeight="178.0" prefWidth="337.0" spacing="10" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">

            <Button fx:id="stackButton" mnemonicParsing="false" onAction="#handleStackButtonAction" prefHeight="26.0" prefWidth="336.0" text="Stack" VBox.vgrow="ALWAYS" />
            <Button fx:id="queueButton" mnemonicParsing="false" onAction="#handleQueueButtonAction" prefHeight="26.0" prefWidth="334.0" text="Queue" VBox.vgrow="ALWAYS" />
            <Button fx:id="listButton" mnemonicParsing="false" onAction="#handleListButtonAction" prefHeight="26.0" prefWidth="335.0" text="List" VBox.vgrow="ALWAYS" />
            <Button fx:id="helpButton" mnemonicParsing="false" onAction="#handleHelpButtonAction" prefHeight="26.0" prefWidth="334.0" text="Help" VBox.vgrow="ALWAYS" />
            <Button fx:id="exitButton" mnemonicParsing="false" onAction="#handleExitButtonAction" prefHeight="26.0" prefWidth="334.0" text="Exit" VBox.vgrow="ALWAYS" />
        </VBox>
    </children>
</AnchorPane>
